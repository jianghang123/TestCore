
@{
    Layout = null;
    var url = ViewBag.url;
}
<link href="~/common/ifream.css" rel="stylesheet" />
<link href="~/common/app.css" rel="stylesheet" />
<link href="~/common/datetimepicker.min.css" rel="stylesheet" />
<div class="right-content">
    <div class="panel panel-default" style="width:400px;margin:auto;margin-top:80px;">
        <div class="panel-heading">
            操作提示：
        </div>
        <div class="panel-body">
            <div style="text-align:center;margin-top:20px;font-size:1.2em" class="red">
                <span class="glyphicon glyphicon-info-sign">
                </span>
                &nbsp;
                设置保存成功
            </div>
            <div style="text-align:center;margin-top:20px;margin-bottom:20px">
                <a href="javascript:void(0);" onclick="homeBack()">
                    管理首页
                </a>
                <span class="v-line">
                    |
                </span>
                <a href="javascript:void(0);" name="@url" onclick="goBack(this)">
                    返回上页
                </a>
            </div>
        </div>
    </div>
</div>
<script src="~/common/jquery-1.12.1.min.js" type="text/javascript"></script>
<script src="~/common/jquery.zclip.min.js" type="text/javascript"></script>
<script src="~/common/datetimepicker.min.js" type="text/javascript"></script>
<script src="~/common/bootstrap.min.js"></script>
<script src="~/common/app.js"></script>
